header.header {
  display: flex;
  justify-content: space-between;
  padding: 25px 15px;
  padding: 25px clamp(15px, 6vw, 125px);
  position: relative;
  z-index: 1;
}

@font-face {
  font-family: "Magallanes";
  src: url({{"MagallanesRegular.ttf" | asset_url}}) format("truetype");
}
.main-content {
  padding: 0 !important;
  margin: 0 !important;
  background: #f5f5f5;
}

.theme-button {
  font-family: "Open Sans";
  font-size: 18px;
  line-height: 25px;
  color: #4a4a4a;
  background: #eaeaea;
  border: 1px solid #4a4a4a;
  padding: 12px 32px;
  margin-top: 50px;
  display: inline-block;
  text-align: center;
}
.theme-button.large {
  display: block;
}
.theme-button.purple:hover {
  color: white;
  background-color: #6241bb;
  border-color: #6241bb;
  transition: 0.25s;
}
.theme-button.blue:hover {
  color: white;
  background-color: #3276af;
  border-color: #3276af;
  transition: 0.25s;
}
.theme-button.yellow:hover {
  color: white;
  background-color: #e9c546;
  border-color: #e9c546;
  transition: 0.25s;
}

.d-none {
  display: none !important;
}

.center {
  display: flex;
  justify-content: center;
}

.shape::after {
  content: "";
  position: absolute;
  left: 0;
  top: -90px;
  height: 730px;
  width: 100%;
  background: url({{"hero-background.png"}});
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: cover;
  z-index: 0;
}

#shopify-section-1-Combo-hero-home {
  background-color: transparent;
  margin-bottom: 300px;
  position: relative;
}
#shopify-section-1-Combo-hero-home:after {
  content: "";
  position: absolute;
  background: url({{"hero-shape-trans.svg"}});
  height: 33vw;
  width: 100%;
  bottom: 110px;
  background-position-y: 100%;
  background-repeat: no-repeat;
  background-size: cover;
  left: 0;
  bottom: 0;
  transform: scale(1.2);
}
#shopify-section-1-Combo-hero-home:before {
  content: "";
  position: absolute;
  background: linear-gradient(180deg, #2a6aa3 0%, #2586db 102%);
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
}

#shopify-section-1-Combo-all-packages {
  padding-top: 50px;
}

.home-hero {
  position: relative;
  display: flex;
  width: 100%;
  justify-content: space-between;
  flex-wrap: wrap;
  max-width: 1800px;
  margin: 0 auto;
  z-index: 10;
  padding: 0 clamp(15px, 6vw, 125px);
  margin-top: clamp(35px, 11vw, 150px);
}
.home-hero-left {
  padding-right: 10px;
  width: 100%;
  max-width: 475px;
}
.home-hero-left .home-text {
  font-family: "Open Sans";
  font-size: 17px;
  color: #ffffff;
}
.home-hero-left .home-text strong {
  font-weight: 600;
}
.home-hero-left .home-header .home-title {
  font-family: "Magallanes";
  font-style: normal;
  font-weight: 600;
  font-size: 28px;
  margin-bottom: 0;
  color: #f5f5f5;
}
.home-hero-left .home-header .home-text {
  margin-bottom: 40px;
}
.home-hero-right {
  display: flex;
  flex: 1;
  position: relative;
  width: 750px;
  max-width: 750px;
  transform: translateY(-20%);
}
.home-hero img {
  align-self: center;
}

.products-section {
  padding: 0 clamp(15px, 6vw, 125px);
  position: relative;
  z-index: 100;
  transform: translateY(-165px);
}
.products-section .all-packages-title {
  font-family: "Magallanes";
  font-weight: 600;
  font-size: clamp(26px, 3vw, 34px);
  text-align: center;
  color: #525252;
}
.products-section .all-packages-sub-text {
  text-align: center;
  font-family: Open Sans;
  font-size: 18px;
  text-align: center;
  color: #525252;
}
.products-section .all-packages-sub-text span {
  display: block;
}
.products-section .all-packages-container {
  max-width: 1250px;
  margin: 0 auto;
  padding: 0 25px;
  margin-top: 45px;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.products-section .package-card {
  min-width: 250px;
  display: flex;
  flex-direction: column;
  margin-top: 50px;
  display: flex;
}
.products-section .package-card img {
  max-height: 310px;
  margin: 0 auto;
}
.products-section .package-card.Premium .large:hover {
  background-color: #3676a8;
}
.products-section .package-card.Premium p.fake-center {
  transform: translateX(25px);
}
.products-section .package-card.Premium p.fake-center + p {
  margin: 0 auto;
}
.products-section .package-card.Collagen .large:hover {
  background-color: #6241bb;
}
.products-section .package-card.Joint .large:hover {
  background-color: #e2cc03;
}
.products-section .package-card .title p {
  position: relative;
  font-family: "Open Sans";
  font-weight: 600;
  font-size: 20px;
  padding-bottom: 20px;
  margin: 5px;
  color: #636363;
}
.products-section .package-card .title p:last-of-type {
  font-size: 16px;
  display: block;
  font-weight: 400;
  max-width: 295px;
  margin: 0 auto;
  text-align: left;
  margin-left: 5px;
}
.products-section .package-card .title p.starts::after {
  content: "";
  position: absolute;
  background: url({{"stars.svg"}});
  height: 15px;
  width: 88px;
  left: 0;
  top: 30px;
}
.products-section .package-card .card-footer {
  margin-top: auto;
}
.products-section .package-card .large {
  width: 100%;
  padding: 5px;
  font-size: 16px;
  max-width: 310px;
  margin: 10px auto;
}
.products-section .package-card .large:hover {
  color: #ffffff;
}
.products-section .package-card .price {
  padding-top: 25px;
}
.products-section .package-card .price p {
  font-family: "Open Sans";
  font-style: normal;
  font-weight: 600;
  font-size: 18px;
  color: #3676a8;
  text-align: center;
  width: 100%;
}

.all-bottles {
  max-width: 1650px;
  margin: 45px auto 65px auto;
  padding: 0 25px;
}
.all-bottles .bottles-container {
  display: flex;
  justify-content: space-evenly;
  flex-wrap: wrap;
}
.all-bottles .bottle-card {
  width: 310px;
  display: flex;
  flex-direction: column;
  margin-top: 50px;
}
.all-bottles .bottle-card-img {
  position: relative;
  display: flex;
  justify-content: center;
}
.all-bottles .bottle-card-img:hover .overlay-bottle {
  z-index: 0;
  opacity: 1;
}
.all-bottles .bottle-card-img .overlay-bottle {
  position: absolute;
  height: 95%;
  width: 100%;
  background-color: rgba(255, 255, 255, 0.65);
  padding: 35px;
  opacity: 0;
  z-index: -1;
  transition: opacity 1s;
}
.all-bottles .bottle-card-img .overlay-bottle p {
  font-size: 16px;
  line-height: 18px;
}
.all-bottles .bottle-card-img .overlay-bottle p:first-of-type {
  font-weight: 700;
}
.all-bottles .bottle-card-img img {
  width: 245px;
  height: 425px;
}
.all-bottles .bottle-card-info {
  margin-top: -20px;
}
.all-bottles .bottle-card-info p {
  font-family: "Open Sans";
  line-height: 1;
  margin: 0;
  font-size: 18px;
}
.all-bottles .bottle-card-info p.tags {
  margin: 5px 0 10px;
  font-family: "Open Sans";
  font-size: 16px;
  color: #b1b1b1;
}
.all-bottles .bottle-card .button-container {
  margin: 25px 0;
}
.all-bottles .bottle-card .button-container .theme-button {
  width: 100%;
  color: #fff;
}
.all-bottles .bottle-card .button-container .theme-button.blue {
  background-color: #3276af;
}
.all-bottles .bottle-card .button-container .theme-button.bronze {
  background-color: #e2cc03;
}
.all-bottles .bottle-card .button-container .theme-button.purple {
  background-color: #6241bb;
}

.button-container {
  margin: 75px 0;
}
.button-container .theme-button {
  margin: 0 auto;
  width: 320px;
}

.reviews-section {
  background-color: #fff;
  min-height: 500px;
  padding: 95px 0 20px 0;
}
.reviews-section h2 {
  font-family: "Magallanes";
  text-align: center;
  margin-bottom: 55px;
  font-weight: 600;
  font-size: 34px;
  color: #000000;
}
.reviews-section .slick-container {
  width: 80%;
  max-width: 1024px;
  margin: 0 auto;
}
.reviews-section .review {
  width: 305px;
  height: 200px;
  background: #f5f5f5;
  border-radius: 10px;
  margin: 10px;
  color: white;
  text-align: center;
}
.reviews-section .trust-line {
  margin-top: 25px;
  display: flex;
  justify-content: center;
}

@media screen and (max-width: 1200px) {
  #shopify-section-1-Combo-hero-home {
    margin-bottom: 160px;
  }
  #shopify-section-1-Combo-hero-home .home-hero {
    flex-direction: column;
    align-items: center;
  }
  #shopify-section-1-Combo-hero-home .home-hero-left {
    max-width: 100%;
    text-align: center;
  }
  #shopify-section-1-Combo-hero-home .home-hero-right {
    margin-top: 140px;
    width: 100%;
  }
  #shopify-section-1-Combo-hero-home .home-hero-right img {
    transform: translateY(50px);
  }
  #shopify-section-1-Combo-hero-home .products-section .package-card .title p {
    text-align: left;
  }
  #shopify-section-1-Combo-hero-home .products-section .package-card .title p.fake-center {
    text-align: initial;
    transform: none;
  }
  #shopify-section-1-Combo-hero-home .products-section .package-card .title p.starts {
    padding-bottom: 10px;
  }
  #shopify-section-1-Combo-hero-home .products-section .package-card .title p.starts:after {
    margin-left: 0;
    top: 30px;
    left: 0;
  }
}
.footer-section {
  width: 100%;
  height: 300px;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: column;
}

.footer-logo-container {
  width: 100%;
  padding-top: 70px;
  display: flex;
  justify-content: center;
}

.footer-social-links {
  width: 100%;
  margin-top: 50px;
  display: flex;
  justify-content: center;
}

.social-icons {
  margin: 0 30px;
}

.footer-other-links {
  padding: 10px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-content: center;
}
.footer-other-links p {
  color: black;
  font-weight: 400;
}
.footer-other-links p span {
  font-weight: 900;
}
.footer-other-links .trustpilot-footer {
  margin-right: 10px;
}

@media screen and (max-width: 360px) {
  .footer-other-links {
    flex-direction: column;
  }
  .footer-other-links p {
    text-align: center;
    margin-top: 10px;
  }
}
.checkout-container {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  max-width: 1800px;
  margin: 0 auto;
  padding-bottom: 60px;
}

.product-hero {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url({{ 'product-background.svg' }});
}
.product-hero-img {
  flex: 1;
  min-width: 725px;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.product-hero-img img {
  max-height: 530px;
  transform: scale(2) translate(0%, -27%);
}

.direct-checkout {
  margin: 0 auto;
  max-width: 525px;
  width: 100%;
  padding: 0 clamp(15px, 6vw, 125px) 30px;
}
.direct-checkout-header {
  max-width: 450px;
  margin-top: clamp(2px, 6vw, 100px);
}
.direct-checkout-header h1 {
  font-family: "Magallanes";
  font-weight: 600;
  font-size: clamp(26px, 3vw, 28px);
  color: #3276af;
  text-transform: uppercase;
  margin-bottom: 5px;
}
.direct-checkout-header p {
  color: #515151;
  font-family: "Open Sans";
}
.direct-checkout-header p.from_price {
  font-family: "Magallanes";
  font-weight: 600;
  font-size: 18px;
  margin: 0;
}
.direct-checkout-body {
  display: flex;
  flex-direction: column;
  padding: 20px 15px 20px 0;
}
.direct-checkout-body .checkout-button {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 425px;
  min-height: 50px;
  background: #ffffff;
  border: 1px solid #4a4a4a;
  border-radius: 10px;
  margin-bottom: 8px;
  padding: 10px 15px;
  cursor: pointer;
}
.direct-checkout-body .checkout-button:first-of-type:after {
  position: absolute;
  content: "";
  width: 60px;
  height: 65px;
  right: 0;
  background-image: url({{"top-seller.svg"}});
  background-size: cover;
  right: 90px;
  top: 0;
}
.direct-checkout-body .checkout-button:hover p {
  color: #ffffff;
}
.direct-checkout-body .checkout-button:hover p:after {
  color: #ffffff;
}
.direct-checkout-body .checkout-button:hover .green {
  color: #40a737 !important;
}
.direct-checkout-body .checkout-button.selected p {
  color: #ffffff;
}
.direct-checkout-body .checkout-button.selected p:after {
  color: #ffffff;
}
.direct-checkout-body .checkout-button.selected .green {
  color: #40a737 !important;
}
.direct-checkout-body .checkout-button.single-month .month-price p:after {
  content: "";
}
.direct-checkout-body .checkout-button.single-month span {
  display: none;
}
.direct-checkout-body .checkout-button.single-month span.single {
  display: inline;
}
.direct-checkout-body .checkout-button .price_info {
  flex: 1;
  max-width: 280px;
}
.direct-checkout-body .checkout-button .price_info p {
  font-family: "Open Sans";
  font-size: 16px;
  font-weight: 700;
}
.direct-checkout-body .checkout-button .price_info .green {
  color: #005128;
  font-weight: 600;
}
.direct-checkout-body .checkout-button .price_info .single {
  display: none;
}
.direct-checkout-body .checkout-button .price_info .cross {
  text-decoration: line-through;
  font-weight: 100;
}
.direct-checkout-body .checkout-button .price_info .save {
  display: block;
  font-size: 18px;
}
.direct-checkout-body .checkout-button .month-price {
  min-width: 76px;
}
.direct-checkout-body .checkout-button .month-price p {
  font-family: "Open Sans";
  font-weight: 600;
  font-size: 18px;
  text-align: center;
  margin: 0;
  position: relative;
}
.direct-checkout-body .checkout-button .month-price p:after {
  content: "per month";
  font-size: 14px;
  position: absolute;
  left: 4px;
  bottom: -12px;
}
.direct-checkout-link {
  width: 100%;
  max-width: 425px;
  margin-top: 10px;
}
.direct-checkout-link .theme-button {
  display: block;
  margin-top: 0;
  background: #40a737;
  color: #ffffff;
  font-family: "Open Sans";
  font-size: 19px;
  color: #ffffff;
  min-width: 425px;
}
.direct-checkout-link .payment {
  width: 100%;
  height: 50px;
  margin-top: 10px;
  display: flex;
  justify-content: center;
}
.direct-checkout-link .payment img {
  height: 100%;
  width: 75%;
}

.product-info {
  position: relative;
  padding: 0 clamp(10px, 6vw, 60px);
  min-height: 750px;
  padding-bottom: 5vw;
}
.product-info-header {
