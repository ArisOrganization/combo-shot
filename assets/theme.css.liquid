header.header {
  display: flex;
  justify-content: space-between;
  padding: 25px 15px;
  padding: 25px clamp(15px, 6vw, 125px);
  position: relative;
  z-index: 10;
}
header.header .all_products {
  display: flex;
  align-items: center;
  margin-left: auto;
  margin-right: 30px;
}
header.header .all_products a {
  font-size: 20px;
  color: #525252;
}
header.header .all_products .theme-button {
  margin-top: -15px;
  border-radius: 20px;
  color: #fff;
  padding: 3px 25px 0px 25px;
}
header.header.light .all_products a {
  color: #fff;
}

@media screen and (max-width: 750px) {
  header.header a:first-of-type {
    order: 2;
  }
  header.header a:last-of-type {
    order: 1;
    height: 35px;
  }
  header.header div.all_products {
    order: 3;
    margin-left: 0;
    margin-right: 0;
  }
  header.header div.all_products a {
    font-size: 14px;
    font-weight: 700;
  }
  header.header div.all_products .theme-button {
    font-size: 11px;
    padding: 3px 10px 0px 10px;
  }
}
@font-face {
  font-family: "Magallanes";
  src: url({{"MagallanesRegular.ttf" | asset_url}}) format("truetype");
}
html {
  scroll-behavior: smooth;
}

.main-content {
  padding: 0 !important;
  margin: 0 !important;
  background: #f5f5f5;
}

.theme-button {
  font-family: "Open Sans";
  font-size: 18px;
  line-height: 25px;
  color: #4a4a4a;
  background: #eaeaea;
  border: 1px solid #4a4a4a;
  padding: 12px 32px;
  margin-top: 50px;
  display: inline-block;
  text-align: center;
}
.theme-button.large {
  display: block;
}
.theme-button.green {
  color: white;
  background-color: #40a737;
  border-color: #40a737;
  transition: 0.25s;
}
.theme-button.green:hover {
  color: #4a4a4a;
  background: #eaeaea;
  border: 1px solid #4a4a4a;
}
.theme-button.purple:hover {
  color: white;
  background-color: #6241bb;
  border-color: #6241bb;
  transition: 0.25s;
}
.theme-button.blue:hover {
  color: white;
  background-color: #3276af;
  border-color: #3276af;
  transition: 0.25s;
}
.theme-button.yellow:hover {
  color: white;
  background-color: #e9c546;
  border-color: #e9c546;
  transition: 0.25s;
}

.d-none {
  display: none !important;
}

.center {
  display: flex;
  justify-content: center;
}

.choose-package {
  margin-top: 130px;
}
.choose-package .first-header {
  display: none;
}

.slick-prev:before,
.slick-next:before {
  height: 20px;
  width: 20px;
  background-image: url(slick-arrow.svg);
  background-size: contain;
  color: #ffffff00 !important;
  background-repeat: no-repeat;
}

.slick-next:before {
  background-image: url(slick-arrow-right.svg);
}

.online-shop {
  height: 100vh;
  overflow-y: scroll;
}
.online-shop .choose-package {
  position: relative;
  z-index: 10;
}
.online-shop #shopify-section-1-Combo-all-bottles {
  padding-top: 70px;
}
.online-shop #shopify-section-1-Combo-all-packages {
  padding-top: 100px;
}
